<Application x:Class="Wpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="NewWindow.xaml"
             DispatcherUnhandledException="App_DispatcherUnhandledException" >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View\ResourceDictionarys\Button.xaml" />
                <ResourceDictionary Source="View\ResourceDictionarys\DataGrid.xaml" />
                <ResourceDictionary Source="View\ResourceDictionarys\Grid.xaml" />
                <ResourceDictionary Source="View\ResourceDictionarys\TabControl.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
